<h1><%=@doctor.name%>'s Information</h1>

<h2>Specialty: <%=@doctor.specialty%></h2>
<h3>Doctorate Recieved From: <%=@doctor.university%></h3>

<h2>Practices at: <%=@doctor.hospital.name%></h2>

<h2>Patients</h2>
<% @doctor.patients.each do |patient| %>
  <section id="patient_<%=patient.id%>">
    <p><%=patient.name%></p><%= button_to("Remove this Patient", method: "delete", url:"/doctors/#{@doctor.id}/patients/#{patient.id}") %>
  </section>
<%end%>
